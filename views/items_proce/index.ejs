<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Produto</th>
            <th>Procedimento</th>
            <th>Quantidade</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        <% itemsProce.forEach(item => { %>
            <tr>
                <td><%= item.cod %></td>
                <td><%= item.produto_nome %></td>
                <td><%= item.procedimento_nome %></td>
                <td><%= item.quantidade %></td>
                <td>
                    <a href="/items_proce/<%= item.cod %>" class="btn btn-info">Ver</a>
                    <a href="/items_proce/<%= item.cod %>/edit" class="btn btn-warning">Editar</a>
                    <form action="/items_proce/<%= item.cod %>?_method=DELETE" method="POST" style="display:inline;">
                        <button class="btn btn-danger">Excluir</button>
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
